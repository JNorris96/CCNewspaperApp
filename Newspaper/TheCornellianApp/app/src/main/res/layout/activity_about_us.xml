<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main"
    android:background="@color/colorPrimary"
    android:id="@+id/relativeLayout">


    <ImageView
        android:id="@+id/cornellianHead"
        android:layout_width="wrap_content"
        android:layout_height="196dp"
        android:contentDescription="@string/cornellianHead"
        android:scaleType="fitStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/cornellian_head"
        tools:layout_conversion_absoluteHeight="357dp"
        tools:layout_conversion_absoluteWidth="384dp" />

    <Button
        android:id="@+id/MainButton"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:background="@color/Background"
        android:gravity="center"
        android:text="@string/mainButtonText"
        android:textColor="@color/colorPrimary"
        android:textColorLink="@color/colorAccent"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/IntroHeader"
        app:layout_constraintEnd_toStartOf="@+id/AboutUsButton"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp"
        android:layout_width="0dp"
        android:layout_weight="1" />

    <Button
        android:id="@+id/AboutUsButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:layout_weight="1"
        android:background="@color/Background"
        android:gravity="center"
        android:text="@string/aboutUsButtonText"
        android:textColor="@color/colorAccent"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/IntroHeader"
        app:layout_constraintEnd_toStartOf="@+id/ContactUsButton"
        app:layout_constraintStart_toEndOf="@+id/MainButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintVertical_chainStyle="packed"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp" />

    <Button
        android:id="@+id/ContactUsButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:layout_weight="1"
        android:background="@color/Background"
        android:gravity="center"
        android:text="@string/contactUsButtonText"
        android:textColor="@color/colorPrimary"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/IntroHeader"
        app:layout_constraintEnd_toStartOf="@+id/ArchiveButton"
        app:layout_constraintStart_toEndOf="@+id/AboutUsButton"
        app:layout_constraintTop_toTopOf="@+id/cornellianHead"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="88dp" />

    <Button
        android:id="@+id/ArchiveButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="124dp"
        android:layout_weight="1"
        android:background="@color/Background"
        android:gravity="center"
        android:text="@string/archiveButtonText"
        android:textColor="@color/colorPrimary"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/IntroHeader"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/ContactUsButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="120dp" />

    <TextView
        android:id="@+id/IntroHeader"
        android:layout_width="107dp"
        android:layout_height="24dp"
        android:layout_marginTop="16dp"
        android:gravity="center_vertical"
        android:text="@string/introHeader"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        app:fontFamily="serif"
        app:layout_constraintBottom_toTopOf="@+id/Introduction"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AboutUsButton"
        tools:layout_conversion_absoluteHeight="26dp"
        tools:layout_conversion_absoluteWidth="99dp" />

    <TextView
        android:id="@+id/Introduction"
        android:layout_width="333dp"
        android:layout_height="143dp"
        android:layout_marginTop="16dp"
        android:gravity="center_vertical"
        android:text="@string/introduction"
        android:textColor="@android:color/black"
        android:textSize="12sp"
        app:fontFamily="serif"
        app:layout_constraintBottom_toTopOf="@+id/EditorialHeader"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/IntroHeader"
        tools:ignore="MissingConstraints"
        tools:layout_conversion_absoluteHeight="140dp"
        tools:layout_conversion_absoluteWidth="358dp" />

    <TextView
        android:id="@+id/EditorialHeader"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="24dp"
        android:gravity="center_vertical"
        android:text="@string/editorHeader"
        android:textColor="@android:color/black"
        android:textSize="16sp"
        app:fontFamily="serif"
        app:layout_constraintBottom_toTopOf="@+id/EditorChief"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Introduction"
        app:layout_constraintVertical_chainStyle="packed"
        tools:layout_conversion_absoluteHeight="22dp"
        tools:layout_conversion_absoluteWidth="151dp" />

    <TextView
        android:id="@+id/EditorChief"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:gravity="center_vertical"
        android:text="@string/editorChief"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/AssistantEditorChief"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditorialHeader"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/AssistantEditorChief"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/assistantEditorChief"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/NewsEditor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/EditorChief"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/NewsEditor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/newsEditor"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/FeaturesEditor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AssistantEditorChief"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/FeaturesEditor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/featuresEditor"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/ArtEntertainmentEditor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/NewsEditor"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/ArtEntertainmentEditor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/artEntertainmentEditor"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/OpinionEditor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FeaturesEditor"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/OpinionEditor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/opinionEditor"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/CopyEditor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ArtEntertainmentEditor"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/CopyEditor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/copyEditor"
        android:textColor="@android:color/black"
        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toTopOf="@+id/LayoutEditor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/OpinionEditor"
        tools:layout_conversion_absoluteHeight="16dp"
        tools:layout_conversion_absoluteWidth="250dp" />

    <TextView
        android:id="@+id/LayoutEditor"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginBottom="32dp"
        android:gravity="center"
        android:text="@string/layoutEditor"
        android:textColor="@android:color/black"

        android:textColorLink="#523178"
        android:textSize="12sp"
        android:typeface="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CopyEditor"
        tools:layout_conversion_absoluteHeight="21dp"
        tools:layout_conversion_absoluteWidth="250dp" />

</android.support.constraint.ConstraintLayout>